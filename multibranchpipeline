@Library('library')_
node('built-in')
{
    stage('ContinuousDownload_Master')
    {
        cicd.newGit("maven")
    }
    stage('ContinuousBuild_Master')
    {
        cicd.newMaven()
    }
    stage('ContinuousDeployment_Master')
    {
        cicd.newDeploy("ScriptedPipelineSharedLibraries","172.31.13.241","testapp")
    }
    stage('ContinuousTesting_Master')
    {
        cicd.newGit("functionaltesting")
        cicd.runSelenium("ScriptedPipelineSharedLibraries")
    }
    stage('ContinuousDelivery_Master')
    {
        cicd.newDeploy("ScriptedPipelineSharedLibraries","172.31.10.80","prodapp")
    }
}
